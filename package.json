{
	"name": "oslo",
	"type": "module",
	"version": "0.9.0",
	"description": "A collection of auth-related utilities",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"format": "prettier -w .",
		"lint": "eslint src"
	},
	"files": [
		"/dist/"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./cookie": {
			"import": "./dist/cookie/index.js",
			"types": "./dist/cookie/index.d.ts"
		},
		"./encoding": {
			"import": "./dist/encoding/index.js",
			"types": "./dist/encoding/index.d.ts"
		},
		"./oauth2": {
			"types": "./dist/oauth2/index.js",
			"import": "./dist/oauth2/index.d.ts"
		},
		"./oauth2/providers": {
			"types": "./dist/oauth2/providers/index.js",
			"import": "./dist/oauth2/providers/index.d.ts"
		},
		"./oidc": {
			"types": "./dist/oidc/index.js",
			"import": "./dist/oidc/index.d.ts"
		},
		"./otp": {
			"types": "./dist/otp/index.js",
			"import": "./dist/otp/index.d.ts"
		},
		"./password": {
			"types": "./dist/password/index.js",
			"import": "./dist/password/index.d.ts"
		},
		"./random": {
			"types": "./dist/random/index.js",
			"import": "./dist/random/index.d.ts"
		},
		"./request": {
			"types": "./dist/request/index.js",
			"import": "./dist/request/index.d.ts"
		},
		"./session": {
			"types": "./dist/session/index.js",
			"import": "./dist/session/index.d.ts"
		},
		"./token": {
			"types": "./dist/token/index.js",
			"import": "./dist/token/index.d.ts"
		},
		"./webauthn": {
			"types": "./dist/webauthn/index.js",
			"import": "./dist/webauthn/index.d.ts"
		}
	},
	"keywords": [
		"auth",
		"oauth2",
		"session",
		"crypto"
	],
	"author": "pilcrowOnPaper",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/pilcrowOnPaper/oslo"
	},
	"devDependencies": {
		"@types/node": "^20.8.6",
		"@typescript-eslint/eslint-plugin": "^6.7.5",
		"@typescript-eslint/parser": "^6.7.5",
		"eslint": "^8.51.0",
		"prettier": "^3.0.3",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"@node-rs/argon2": "^1.5.2",
		"@node-rs/bcrypt": "^1.7.3"
	}
}
